<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <solid android:color="#ffffff" />
            <stroke
               
                android:dashWidth="2dp"
                android:width="2dp"
                android:color="#ed207b" />
            <padding
                android:bottom="3dp"
                android:left="3dp"
                android:right="3dp"
                android:top="3dp" />
            <corners android:radius="15dp" />
        </shape>
    </item>
</layer-list>